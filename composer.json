{
    "require": {
        "php": ">=5.3",
        "composer\/installers": "1.x",
        "mnsami\/composer-custom-directory-installer": "1.1.*",
        "webdevstudios\/cmb2": "2.3.0"
    },
    "repositories": {
        "2": {
            "type": "composer",
            "url": "https:\/\/wpackagist.org"
        }
    },
    "config": {
        "vendor-dir": "composer\/"
    },
    "require-dev": {
        "phpro\/grumphp": "^0.12",
        "lucatume\/function-mocker": "~1.0"
    },
    "minimum-stability": "dev",
    "extra": {
        "installer-paths": {
            ".\/includes\/lib\/{$name}": [
                "johnbillion\/extended-cpts",
                "wpbp\/language",
                "wpbp\/cronplus",
                "wpbp\/fakepage",
                "wpbp\/template",
                "wpbp\/requirements",
                "wpbp\/debug",
                "wpackagist-plugin\/posts-to-posts"
            ],
            ".\/includes\/lib\/Freemius": [
                "freemius\/wordpress-sdk"
            ],
            ".\/public\/includes\/lib\/{$name}": [
                "wpbp\/widgets-helper"
            ],
            ".\/admin\/includes\/lib\/{$name}": [
                "webdevstudios\/cmb2",
                "yoast\/i18n-module",
                "origgami\/cmb2-grid",
                "rubengc\/cmb2-tabs",
                "wpbp\/pointerplus",
                "wpbp\/cpt_columns",
                "wpbp\/backbone-modal-view",
                "kevinlangleyjr\/wp-contextual-help",
                "nathanielks\/wp-admin-notice",
                "julien731\/wp-dismissible-notices-handler",
                "julien731\/wp-review-me",
                "yoast\/whip"
            ]
        }
    }
}